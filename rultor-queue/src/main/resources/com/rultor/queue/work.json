{
    "description": "work",
    "type":"object",
    "required": ["urn", "unit", "scheduled"],
    "additionalProperties": false,
    "properties": {
        "urn": {
            "type": "string",
            "pattern": "^urn:[a-z]+:[0-9]+$"
        },
        "unit": {
            "type": "string",
            "pattern": "^[a-z0-9\\-]+$"
        },
        "scheduled": {
            "type": "string",
            "format": "date-time"
        }
    }
}
